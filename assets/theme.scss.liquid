/*================ Add to cart form ================*/
.product-form {
  // @include display-flexbox();
  // @include flex-wrap(wrap);
  // @include align-items(flex-end);
  width: auto;
  margin: 0 -5px -10px;

  .flex-wrapper {
    @include display-flexbox();
    @include flex-wrap(wrap);
    @include align-items(flex-end);
  }

  label {
    font-size: 15px;
    font-weight: bold;
  }
}

.product-form__item {
  @include flex(1 1 200px);
  margin-bottom: 10px;

  label {
    display: block;
    
    .product-form--hide-variant-labels & {
      // sass-lint:disable no-important
      position: absolute !important;
      overflow: hidden;
      clip: rect(0 0 0 0);
      height: 1px;
      width: 1px;
      margin: -1px;
      padding: 0;
      border: 0;
    }
  }


}

.product-form__variants {
  display: none;

  .no-js & {
    display: block;
  }
}

.product-form__item--quantity {
  @include flex(0 0 150px);
}

.product-form__item--submit {
  @include flex-basis(200px);
}

.product-form__input {
  display: block;
  width: 100%;
}

.product-form__cart-submit {
  display: block;
  width: 60%;
  margin: 0 auto;
  line-height: 1.4;
  padding-left: 5px;
  padding-right: 5px;
  white-space: normal;
  position: relative;
  background-color: #f03534;
  overflow: hidden;

  &.select_size {
    background-color: #1175cb;
  }

  &:after {
    content: '';
    position: absolute;
    left: -100%;
    width: 100%;
    height: 100%;
    top: 0px;
    padding: 8px;
    border-radius: 3px;
    background-image: -webkit-linear-gradient(left, rgba(255,255,255,0) 0%, #fff 50%, rgba(255,255,255,0) 100%);
    background-image: linear-gradient(to right, rgba(255,255,255,0) 0%, #fff 50%, rgba(255,255,255,0) 100%);
    -webkit-animation: sheen 2s ease 0.5s infinite;
    animation: sheen 2s ease 0.5s infinite;
    opacity: 0.2;
  };
}

@include media-query($medium-up) {
  .product-form__cart-submit--small {
    // max-width: 300px;
  }
}

.product-single__description {
  margin-top: $grid-gutter * 1.5;
}

