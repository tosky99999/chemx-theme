/*============================================================================
  #Cart & Nav Drawers
==============================================================================*/
/*============================================================================
  Layer promotion mixin for creating smoother animations with higher FPS.
==============================================================================*/
@mixin promote-layer($properties: transform) {
  -webkit-transform: translateZ(0); // translateZ hack
  will-change: $properties; // spec
}
.js-drawer-open {
  overflow: hidden;
}

.drawer {
  @include promote-layer();
  display: none;
  position: fixed;
  overflow-y: auto;
  overflow-x: hidden;
  -webkit-overflow-scrolling: touch;
  top: 0;
  bottom: 0;
  max-width: 90%;
  z-index: $z-index-drawer;
  color: $colorDrawerText;
  background-color: rgba(255, 255, 255, 0.96);
  transition: $drawerTransition;

  a {
    color: $colorDrawerText;

    &:hover,
    &:focus {
      opacity: 0.7;
    }
  }

  input,
  textarea {
    border-color: $colorDrawerBorder;
  }
}

.drawer--left {
  width: $drawerNavWidth;
  left: -$drawerNavWidth;
  border-right: 1px solid $colorDrawerBorder;

  

  .js-drawer-open-left & {
    display: block;
    @include transform(translateX($drawerNavWidth));

    .lt-ie9 & {
      left: 0;
    }
  }
}

.drawer--right {
  width: $drawerCartWidth;
  right: -$drawerCartWidth;
  border-left: 1px solid $colorDrawerBorder;

  .js-drawer-open-right & {
    display: block;
    @include transform(translateX(-$drawerCartWidth));

    .lt-ie9 & {
      right: 0;
    }
  }
}

#PageContainer {
  // overflow: hidden;
}

.is-moved-by-drawer {
  @include promote-layer();
  transition: $drawerTransition;

  .js-drawer-open-left & {
    @include transform(translateX($drawerNavWidth));
  }

  .js-drawer-open-right & {
    @include transform(translateX(-$drawerCartWidth));
  }
}

.js-drawer-open .drawer-page-content:after {
  visibility: visible;
  opacity: 1;
}

.drawer-page-content:after {
  visibility: hidden;
  opacity: 0;
  content: '';
  display: block;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.35);
  z-index: 14;
  transition: all 0.35s cubic-bezier(0.29, 0.63, 0.44, 1);
}

.drawer__header {
  position: relative;
  display: table;
  height: 70px;
  width: 100%;
  padding: 0 15px;
  background: #fafafa;
  border-bottom: 2px solid #f0f0f0;
}

.table-cell {
  display: table-cell;
  vertical-align: middle;
  width: 100%;
}

.table-cell.drawer__header__left {
  position: absolute; 
  left: 0;
  top: 50%;
  width: auto;
  transform: translate(100%, -38%);
}

.drawer__header h3 {
  width: 100%;
  margin-bottom: 0;
  font: 700 22px/22px "Helvetica", sans-serif;
  text-transform: uppercase;
  text-align: center;
}

.drawer__title,
.drawer__close {
  display: table-cell;
  vertical-align: middle;
}

.drawer__title {
  width: 100%;
}

.drawer__close {
  cursor: pointer;
}

.drawer__close button {
  position: relative;
  height: 100%;
  padding: 0 20px;
  color: inherit;

  &:active,
  &:focus {
    background-color: darken($colorDrawers, 5%);
  }
}

/*================ Quantity Selectors ================*/
.js-qty {
  position: relative;
  margin-bottom: 1em;
  overflow: visible;

  input {
    float: left;
    display: block;
    background: transparent;
    text-align: center;
    width: 100%;
    max-width: 50px;
    height: 50px;
    color: #000000;
    font-size: 14px;
    font-weight: 400;
    line-height: 50px;
    padding: 0;
    border-top: 1px solid $color-border;
    border-bottom: 1px solid $color-border;
    border-right: 0;
    border-left: 0;
    border-radius: 0;
    margin: 0;
  }
}

.js-qty__adjust {
  cursor: pointer;
  display: block;
  // width: 50px;
  height: 50px;
  line-height: 50px;
  font-size: 14px;
  font-weight: 400;
  color: #000000;
  border: 0 none;
  padding: 0 8px;
  background: transparent;
  text-align: center;
  overflow: hidden;
  outline: none;
  @include user-select(none);

  &:hover{
    color: #fff;
    background-color: #333;
  }
}

.js-qty__adjust--plus {
  float: left;
  border: 1px solid $color-border;
  // border-radius: 0 20px 20px 0;
}

.js-qty__adjust--minus {
  float: left;
  border: 1px solid $color-border;
  // border-radius: 20px 0 0 20px;
}

/*================ Quantity Selectors in Ajax Cart ================*/
.ajaxcart__qty {
  @extend .js-qty;
  margin: 0;

  .is-loading & {
    opacity: 0.5;
    transition: none;
  }
}

.ajaxcart__qty-num {
  border-color: $colorDrawerBorder;
  color: $colorDrawerText;
}

.ajaxcart__qty-adjust {
  @extend .js-qty__adjust;
  float: left;
  color: $colorDrawerText;
}

.ajaxcart__qty-adjust.ajaxcart__qty--minus {
  border: 1px solid $color-border;
  // border-radius: 20px 0 0 20px;
  // width: 33px;
  height: 33px;
  line-height: 33px;
}

.ajaxcart__qty-num {
  width: 42px !important;
  height: 33px !important;
  line-height: 33px !important;
}

.ajaxcart__qty-adjust.ajaxcart__qty--plus {
  border: 1px solid $color-border;
  // border-radius: 0 20px 20px 0;
  // width: 33px;
  height: 33px;
  line-height: 33px;
}

.ajaxcart-item__price-strikethrough {
  float: right;
}




