/*============================================
=            IE 10+ & Edge Styles            =
============================================*/

@media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
 /* IE10+ CSS */

  .grid-view-item__image {
    width: auto;
  }
  .slideshow__video .hero__video-wrap video {
    height: auto;

    @media (max-width: 750px) {
      width: auto;
    }
  }

  .slideshow__video .hero__video-wrap .hero__video-content .img-logo {
    margin-right: 0px;
  }
  .slideshow__video .hero__video-wrap .hero__video-content {
    display: block;

    .img-logo {
      margin: 0 auto;
      display: block;
    }

    .img-desktop-txt {
      height: 19%;
    }
  }
}


@supports (-ms-ime-align:auto) {
   /* IE Edge CSS */ 
  .grid-view-item__image {
    width: auto;
  }

  .slideshow__video .hero__video-wrap {
    video {
      height: auto;
    }

    .hero__video-content {
      .img-logo {
        width: auto;
      }
    }
  }

}
@supports (-ms-accelerator:true) {
 /* IE Edge 12+ CSS */ 
  .grid-view-item__image {
    width: auto;
  }

  .slideshow__video .hero__video-wrap video {
    height: auto;
  }
}


