  color: #222;
  text-align: center;
  transform: translate3d(0, -50%, 0);
  transition: opacity 1s ease 0.14s;
}

.drawer__footer {
  flex: none;
  width: 100%;
  height: 72px;
  background: #fafafa;
  border-top: 1px solid #ddd;
  padding: 8px;
  position: absolute;
  bottom: 0;
