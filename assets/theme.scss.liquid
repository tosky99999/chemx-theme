}

.drawer__cart__btn {
  width: calc(100% - 16px);
  height: 52px;
  display: block;
  position: absolute;
  background: #f03534;
  border: 0;
  border-radius: 3px;
  text-align: center;
  color: #fff !important;
  letter-spacing: 0.04em;
  box-shadow: 0 2px 8px 0 rgba(87, 71, 81, 0.3), 0 6px 16px 0 rgba(87, 71, 81, 0);
  transition: background 0.24s, box-shadow 0.24s;
}

.drawer__cart__btn span {
  width: 100%;
  position: absolute;
  top: 50%;
  left: 0;
  transform: translate3d(0, -50%, 0);
  text-align: center;
}

.ajaxcart__inner {
  position: absolute;
  top: 70px;
  bottom: 70px;
  width: 100%;
  overflow: auto;
}

.ajaxcart__row {
  > .grid {
    margin-left: -$grid-gutter / 2;

    > .grid__item {
      padding-left: $grid-gutter / 2;
    }
  }
}

.ajaxcart__product {
  position: relative;
  max-height: 500px;
  
  &.is-removed {
    max-height: 0;
    overflow: hidden;
    visibility: hidden;
    transition: all 450ms cubic-bezier(0.57,.06,.05,.95);
    @include backface();
  }
}

.ajaxcart__row {
  width: 100%;
  padding: 16px;
  margin-bottom: 0;
  background: #ffffff;
  border-bottom: 1px solid #e5e5e5;
}

.ajaxcart__product:hover .sidecart-grid__remove {
  display: block;
}

.sidecart-grid__remove {
  flex: none;
  position: absolute;
  top: 8px;
  right: 8px;
  width: 11px;
  height: 11px;
  -webkit-transition: all 0.24s;
  transition: all 0.24s;
  opacity: 1.0;
  cursor: pointer;
  display: none;
}

.sidecart-grid__remove .x--one {
  display: block;
  position: absolute;
  top: 5px;
  left: 0;
  width: 11px;
  height: 1px;
  background: #444444;
  -webkit-transition: all 0.24s;
  transition: all 0.24s;
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
}

.sidecart-grid__remove .x--two {
  display: block;
  position: absolute;
  top: 5px;
  left: 0;
  width: 11px;
  height: 1px;
  background: #444444;
  -webkit-transition: all 0.24s;
  transition: all 0.24s;
  -webkit-transform: rotate(45deg);
  transform: rotate(45deg);
}

.ajaxcart__product-image {
  display: block;
  overflow: hidden;
  margin-bottom: 15px;

  img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
  }
}

.ajaxcart__product-name,
